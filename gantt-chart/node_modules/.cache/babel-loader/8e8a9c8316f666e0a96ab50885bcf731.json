      sections: ['Section 1', 'Section 2', 'Section 3']\r\n    };\r\n  },\r\n  components: {\r\n    draggable,\r\n  },\r\n  mounted() {\r\n        this.list =  [1,2,3,4,54,5,56,5,21,1,23,4,65,6,2,3,4,5,6,6,7,8,8,90,2,1,1,1,1,1,12,3];\r\n        let element = document.querySelector('#infinite');\r\n        console.log(element)\r\n        element.addEventListener(\"scroll\", this.handleScroll);\r\n    },  \r\n  methods: {\r\n    handleScroll() {\r\n            let e = document.querySelector('#infinite');\r\n            if(e.scrollLeft + e.clientWidth >= e.scrollWidth)\r\n            {\r\n                this.list.push(this.list[this.list.length - 1]+1)\r\n            }\r\n        },\r\n        addedLog(e) {\r\n          e.item._underlying_vm_ = e.newIndex;\r\n          console.log(this.list1)\r\n          console.log(this.list2)\r\n        },\r\n        removedLog(e) {\r\n          console.log(e)\r\n        }\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n.card-height {\r\n  height: 500px;\r\n  /* margin: 80px 0 0 0; */\r\n}\r\n\r\n.card1\r\n{\r\n  padding: 50px 0 0 10px;\r\n  border-top: 1px solid black;\r\n  width: 25%;\r\n\r\n}\r\n.card1::before{\r\n content: \"\";\r\n    display: block;\r\n    position: fixed;\r\n    width:calc(25%);\r\n    background-color: #e1e1e1;\r\n    height: 2.5px;\r\n\r\n}\r\n\r\n.card2\r\n{\r\n  padding: 0;\r\n  width: calc(75% - 50px);\r\n}\r\n.timeline\r\n{\r\n  \r\n  margin: 26px 0 0 0;\r\n  text-align: left;\r\n}\r\n.timeline::after{\r\n content: \"\";\r\n    display: block;\r\n    position: fixed;\r\n    width:calc(75% - 34px);\r\n    background-color: #e1e1e1;\r\n    height: 2.5px;\r\n    z-index: 0;\r\n\r\n}\r\n.section\r\n{\r\n  min-height: 50px;\r\n  padding: 5px 16px 0;\r\n  line-height: 20px;\r\n  margin: 0;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.dragline\r\n{\r\n  min-height: 50px;\r\n  line-height: 49px;\r\n  position: relative;\r\n\r\n}\r\n.dragline::after{\r\n  content: \"\";\r\n    display: block;\r\n    position: fixed;\r\n    width:calc(75% - 34px);\r\n    background-color: #e1e1e1;\r\n    height: 0.5px;\r\n    z-index: 0;\r\n    bottom: 0;\r\n    left: 0;\r\n}\r\n\r\n.element\r\n{\r\ncursor: grab;\r\n}\r\n\r\n.element:active{\r\n  cursor:grabbing\r\n}\r\n\r\n.dragboard\r\n{\r\n  position: relative;\r\n}\r\n.dragline .borderb\r\n{\r\nposition: fixed;\r\nborder-bottom: 1px solid black;\r\nwidth: calc(75%);\r\nleft: 0;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}